<html t:type="input" title="Create Assessment"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
      
<body>
   <t:form t:id="org" class="appnitro">
   
   <div class="form_description">
     <h2>Eastern Region RCSD Organization (ORG) Worksheet</h2>
     <p>Use this form to add or update a ORG record into EPiko (Worsheet Version May 14 2014)</p>
   </div>		
   
   <t:errors/>
   
   <!--  li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('ppBox');" title="Show or Hide">Partner and/or Program</legend>
       <div id="ppBox" class="unhidden">
        <t:checkbox t:id="partner" t:value="org.partner" class="element checkbox" /><t:label for="partner" class="choice"/>
        <t:checkbox t:id="program" t:value="org.program" class="element checkbox" /><t:label for="program" class="choice"/>
       </div>
       
      </fieldset>
     </li -->
   

   <ul>
   
     <li id="li_code">
      <t:label class="description" for="code" />
      <t:textField t:id="code" t:validate="maxlength=255" size="15" value="org.code" />
     </li>
     
     <li id="li_name">
      <t:label class="description" for="name" />
      <div>
       <t:textArea t:id="name" t:validate="maxlength=255" rows="3" cols="90" value="org.name" />
      </div>
     </li>


     <li id="li_contact">
      <t:label class="description" for="contact" />
      <div>
       <t:textArea t:id="contact" t:validate="maxlength=255" rows="2" cols="90" value="org.contact" />
      </div>
     </li>


     <li id="li_email">
      <t:label class="description" for="email" />
      <div>
       <t:textArea t:id="email" t:validate="maxlength=255" rows="2" cols="90" value="org.email" />
      </div>
     </li>

	<li id="li_description">
	<t:label class="description" for="description" />
	<t:textArea t:id="description" t:validate="maxlength=4096" rows="3" cols="90" value="org.description" />
	</li>

	<li id="li_affiliations">
	<t:label class="description" for="affiliations" />
	<t:textArea t:id="affiliations" t:validate="maxlength=255" rows="3" cols="90" value="org.affiliations" />
	</li>

  	<li id="li_url">
	<t:label class="description" for="url" />
	<t:textArea t:id="url" t:validate="maxlength=255" rows="3" cols="90" value="org.url" />
	</li>
	<li id="li_logo">
	<t:label class="description" for="logo" />
	<t:textArea t:id="logo" t:validate="maxlength=255" rows="3" cols="90" value="org.logo" />
	</li>
	<li id="li_worksheet">
	<t:label class="description" for="worksheet" />
	<t:textArea t:id="worksheet" t:validate="maxlength=255" rows="3" cols="90" value="org.worksheet" />
	</li>
	<li id="li_keywords">
	<t:label class="description" for="keywords" />
	<t:textArea t:id="keywords" t:validate="maxlength=4096" rows="3" cols="90" value="org.keywords" />
	</li>

     <li>     
     <fieldset>

      <legend class="description" onclick="javascript:hideToggle('catBox');" title="Show or Hide">Category or Type</legend>
       <div id="catBox" class="unhidden">
		
        <t:checkbox t:id="federal" t:value="org.federal" class="element checkbox" /><t:label for="federal" class="choice" />
        <t:checkbox t:id="province" t:value="org.province" class="element checkbox" /><t:label for="province" class="choice" />
        <t:checkbox t:id="state" t:value="org.state" class="element checkbox" /><t:label for="state" class="choice" />
        <t:checkbox t:id="local" t:value="org.local" class="element checkbox" /><t:label for="local" class="choice" />
        <t:checkbox t:id="interagency" t:value="org.interagency" class="element checkbox" /><t:label for="interagency" class="choice" />
        <t:checkbox t:id="academic" t:value="org.academic" class="element checkbox" /><t:label for="academic" class="choice" />
        <t:checkbox t:id="ngo" t:value="org.ngo" class="element checkbox" /><t:label for="ngo" class="choice" />
        <t:checkbox t:id="otherPartnerType" t:value="org.otherPartnerType" class="element checkbox" /><t:label for="otherPartnerType" class="choice" />
		
       </div>
       
      </fieldset>
     </li>

     <li>
     <fieldset> 
         <legend class="description" onclick="javascript:hideToggle('aoaBox');" title="Show or Hide">Area of Applicability</legend>
       <div id="aoaBox" class="unhidden">

		<t:checkbox t:id="international" t:value="org.international"  class="element checkbox" /><t:label for="international" class="choice" />
		<t:checkbox t:id="canada" t:value="org.canada"  class="element checkbox indent" /><t:label for="canada" class="indent" />
		<t:checkbox t:id="newBrunswick" t:value="org.newBrunswick"  class="element checkbox indent2" /><t:label for="newBrunswick" class="indent2" />
		<t:checkbox t:id="novaScotia" t:value="org.novaScotia"  class="element checkbox indent2" /><t:label for="novaScotia" class="indent2" />
		<t:checkbox t:id="quebec" t:value="org.quebec"  class="element checkbox indent2" /><t:label for="quebec" class="indent2" />
		<t:checkbox t:id="princeEdwardIsland" t:value="org.princeEdwardIsland"  class="element checkbox indent2" /><t:label for="princeEdwardIsland" class="indent2" />
		<t:checkbox t:id="newfoundland" t:value="org.newfoundland"  class="element checkbox indent2" /><t:label for="newfoundland" class="indent2" />
		<t:checkbox t:id="labrador" t:value="org.labrador"  class="element checkbox indent2" /><t:label for="labrador" class="indent2" />
		<t:checkbox t:id="atlanticCanada" t:value="org.atlanticCanada"  class="element checkbox indent" /><t:label for="atlanticCanada" class="indent" />
		<t:checkbox t:id="national" t:value="org.national"  class="element checkbox" /><t:label for="national" class="choice" />
		<t:checkbox t:id="regionalOrState" t:value="org.regionalOrState"  class="element checkbox" /><t:label for="regionalOrState" class="choice" />
		<t:checkbox t:id="gulfOfMaine" t:value="org.gulfOfMaine"  class="element checkbox indent" /><t:label for="gulfOfMaine" class="indent" />
		<t:checkbox t:id="newEngland" t:value="org.newEngland"  class="element checkbox indent" /><t:label for="newEngland" class="indent" />
		<t:checkbox t:id="maine" t:value="org.maine"  class="element checkbox indent2" /><t:label for="maine" class="indent2" />
		<t:checkbox t:id="newHampshire" t:value="org.newHampshire"  class="element checkbox indent2" /><t:label for="newHampshire" class="indent2" />
		<t:checkbox t:id="massachusetts" t:value="org.massachusetts"  class="element checkbox indent2" /><t:label for="massachusetts" class="indent2" />
		<t:checkbox t:id="vermont" t:value="org.vermont"  class="element checkbox indent2" /><t:label for="vermont" class="indent2" />
		<t:checkbox t:id="connecticut" t:value="org.connecticut"  class="element checkbox indent2" /><t:label for="connecticut" class="indent2" />
		<t:checkbox t:id="rhodeIsland" t:value="org.rhodeIsland"  class="element checkbox indent2" /><t:label for="rhodeIsland" class="indent2" />
		<t:checkbox t:id="midAtlantic" t:value="org.midAtlantic"  class="element checkbox indent" /><t:label for="midAtlantic" class="indent" />
		<t:checkbox t:id="newYork" t:value="org.newYork"  class="element checkbox indent2" /><t:label for="newYork" class="indent2" />
		<t:checkbox t:id="newJersey" t:value="org.newJersey"  class="element checkbox indent2" /><t:label for="newJersey" class="indent2" />
		<t:checkbox t:id="pennsylvania" t:value="org.pennsylvania"  class="element checkbox indent2" /><t:label for="pennsylvania" class="indent2" />
		<t:checkbox t:id="marlyland" t:value="org.marlyland"  class="element checkbox indent2" /><t:label for="marlyland" class="indent2" />
		<t:checkbox t:id="delaware" t:value="org.delaware"  class="element checkbox indent2" /><t:label for="delaware" class="indent2" />
		<t:checkbox t:id="virginia" t:value="org.virginia"  class="element checkbox indent2" /><t:label for="virginia" class="indent2" />
		<t:checkbox t:id="districtOfColumbia" t:value="org.districtOfColumbia"  class="element checkbox indent2" /><t:label for="districtOfColumbia" class="indent2" />
		<t:checkbox t:id="central" t:value="org.central"  class="element checkbox indent" /><t:label for="central" class="indent" />
		<t:checkbox t:id="westVirginia" t:value="org.westVirginia"  class="element checkbox indent2" /><t:label for="westVirginia" class="indent2" />
		<t:checkbox t:id="greatLakes" t:value="org.greatLakes"  class="element checkbox indent" /><t:label for="greatLakes" class="indent" />
		<t:checkbox t:id="ohio" t:value="org.ohio"  class="element checkbox indent2" /><t:label for="ohio" class="indent2" />
		<t:checkbox t:id="southEast" t:value="org.southEast"  class="element checkbox indent" /><t:label for="southEast" class="indent" />
		<t:checkbox t:id="northCarolina" t:value="org.northCarolina"  class="element checkbox indent2" /><t:label for="northCarolina" class="indent2" />
		<t:checkbox t:id="southCarolina" t:value="org.southCarolina"  class="element checkbox indent2" /><t:label for="southCarolina" class="indent2" />
		<t:checkbox t:id="localCity" t:value="org.localCity"  class="element checkbox" /><t:label for="localCity" class="choice" />
		<t:checkbox t:id="problemFocused" t:value="org.problemFocused"  class="element checkbox" /><t:label for="problemFocused" class="choice" />


       </div>
   
   
   
      </fieldset>
     </li>

		

	


     <li class="buttons">
       <t:submit class="button_text" value="Add/Update" />
       <t:submit t:id="search" value="Search" />
       (<t:pagelink page="prop:componentResources.pageName">CLEAR - form</t:pagelink>)
       (<t:pagelink page="orgs">CANCEL - go to list</t:pagelink>)
       
     </li>

   </ul>
  </t:form>	
    
 </body>
</html>